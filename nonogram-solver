tf = False
a = []
c = []
while tf != True:
    b = input()
    if b != '/':
        a.append(b)
    elif b == '/':
        tf = True
while tf != True:
    b = input()
    if b != '/':
        c.append(b)
    elif b == '/':
        tf = True
board = [
    [' ',' '],
    [' ',' ']
]
for i in range(len(a)):
    if a[i] == len(board[i]):
        for j in range(len(board[i])):
            board[i][j] = 'O'